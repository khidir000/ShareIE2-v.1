<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="10dp">

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginRight="10dp">
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nim"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Email"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alamat"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tgl Lahir"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="No."
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Konsentrasi"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Angkatan"
                android:textSize="14sp"
                android:textColor="@color/hitam"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/nama_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/nim_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/email_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/alamat_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/tgl_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/no_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/kbk_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/angkatan_pribadi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@color/hitam"
                android:textSize="14sp"/>
        </LinearLayout>
    </LinearLayout>
    <Button
        android:id="@+id/logout"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:text="Logout"
        android:textAllCaps="false"/>
</LinearLayout>